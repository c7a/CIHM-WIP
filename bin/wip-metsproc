#!/usr/bin/env perl

use strict;
use warnings;

use lib "/opt/c7a-perl/current/cmd/local/lib/perl5";
use FindBin;
use lib "$FindBin::RealBin/../lib";

use Sys::RunAlone silent => 1;
use Getopt::Long;
use CIHM::WIP::metsproc;

my $conf = "/etc/canadiana/wip/wip.conf";

GetOptions (
    'conf:s' => \$conf,
    );

CIHM::WIP::metsproc->new({
    configpath => $conf
                         })->run;

1;
__END__
